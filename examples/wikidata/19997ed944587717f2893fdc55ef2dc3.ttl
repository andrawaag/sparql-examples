    ?wikilink schema:about ?human.
    ?wikilink schema:isPartOf <https://eu.wikipedia.org/>.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"eu\" }
}
GROUP BY ?genderLabel
LIMIT 6""" .
