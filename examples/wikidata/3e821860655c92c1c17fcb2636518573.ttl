WHERE {
  ?person wdt:P172 wd:Q726673.
  ?person wdt:P31 wd:Q5.
  OPTIONAL { ?person wdt:P21  ?gender }
  OPTIONAL { ?person wdt:P106 ?occupation }
  OPTIONAL { ?person wdt:P569 ?p569. BIND(SUBSTR(STR(?p569),1,10) as ?birthdate) }
 ...
}
