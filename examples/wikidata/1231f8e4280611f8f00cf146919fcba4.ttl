  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d812848-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d812848-388505537501861C29026C018A2A1D88;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?name ?nameLabel ?count WHERE {
  {
    SELECT ?name (COUNT(?person) AS ?count) WHERE {
      ?person wdt:P31 wd:Q5;
              wdt:P27 wd:Q36;
              wdt:P21 wd:Q6581097;
              wdt:P735 ?name.
    }
    GROUP BY ?name
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\", \"pl\". }
}
ORDER BY DESC(?count)
LIMIT 100""" .
