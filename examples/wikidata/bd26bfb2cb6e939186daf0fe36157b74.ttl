  sh:select """PREFIX schema: <http://schema.org/>
SELECT ?first_letter  (COUNT(?first_letter) as ?count) (?count/?total*100 AS ?pct)

WITH
{ 
  SELECT (COUNT(?first_letter) AS ?total)
  WHERE
  {
    {
  SELECT ?first_letter
  WHERE
  {
    ?article schema:name ?title.
    ?article schema:isPartOf <https://ca.wikipedia.org/>.
    BIND (SUBSTR(?title, 1, 1) AS ?first_letter)
    FILTER (! STRSTARTS(?title, \"Categoria:\"))
    FILTER (! STRSTARTS(?title, \"Plantilla:\"))    
  }
}
  }
} AS %total
WHERE
{
  {
  SELECT ?first_letter
  WHERE
  {
    ?article schema:name ?title.
    ?article schema:isPartOf <https://ca.wikipedia.org/>.
    BIND (SUBSTR(?title, 1, 1) AS ?first_letter)
    FILTER (! STRSTARTS(?title, \"Categoria:\"))
    FILTER (! STRSTARTS(?title, \"Plantilla:\"))    
  }
}
  INCLUDE %total
}
GROUP BY ?first_letter ?total""";
  <http://example.org/bigdata_select> """SELECT ?first_letter  (COUNT(?first_letter) as ?count) (?count/?total*100 AS ?pct)
WITH
{
  SELECT ?first_letter
  WHERE
  {
    ?article schema:name ?title.
    ?article schema:isPartOf <https://ca.wikipedia.org/>.
    BIND (SUBSTR(?title, 1, 1) AS ?first_letter)
    FILTER (! STRSTARTS(?title, \"Categoria:\"))
    FILTER (! STRSTARTS(?title, \"Plantilla:\"))    
  }
} AS %get_letters
WITH
{ 
  SELECT (COUNT(?first_letter) AS ?total)
  WHERE
  {
    INCLUDE %get_letters
  }
} AS %total
WHERE
{
  INCLUDE %get_letters
  INCLUDE %total
}
GROUP BY ?first_letter ?total""" .
