<https://www.wikidata.org/#query-55e2113f7c3b85753dfec2b123444b99> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:SAPA_bdc/SPARQL>;
