<https://www.wikidata.org/#query-e9cbfbe11cd968250430d6613291dbc5> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2018/07>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d823879-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
select ?descendentLabel (count(?mid) as ?distance)  where { 
  ?descendent (wdt:P22|wdt:P25)* ?mid .
  ?mid (wdt:P22|wdt:P25)+ wd:Q9682. 
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
group by ?descendentLabel 
order by ?distance ?descendentLabel""" .
