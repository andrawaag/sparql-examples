  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d861820-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d861820-15AD1F0B89DC2FB57FA963C8BA72A3A4;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?item ?cityLabel ?countryLabel ?population
WHERE
{
	?city wdt:P31/wdt:P279* wd:Q515 . # changed
    ?city wdt:P17 wd:Q212 .
    ?city wdt:P17 ?country .
    ?city wdt:P1082 ?population .
    FILTER (?population >= 1000000)
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
ORDER BY DESC(?population)""" .
