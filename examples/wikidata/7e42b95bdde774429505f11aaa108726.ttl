<https://www.wikidata.org/#query-7e42b95bdde774429505f11aaa108726> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Synia:license-index>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d814594-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT
  ?count
  ?license ?licenseLabel
  (CONCAT(\"#license/\", SUBSTR(STR(?license), 32)) AS ?licenseUrl)
  ?licenseDescription

WHERE {
  {
  SELECT 
    (COUNT(*) AS ?count)
    ?license
  WHERE {
    [] wdt:P275 ?license .
  } 
  GROUP BY ?license
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """SELECT
  ?count
  ?license ?licenseLabel
  (CONCAT(\"#license/\", SUBSTR(STR(?license), 32)) AS ?licenseUrl)
  ?licenseDescription
WITH {
  SELECT 
    (COUNT(*) AS ?count)
    ?license
  WHERE {
    [] wdt:P275 ?license .
  } 
  GROUP BY ?license
} AS %licenses
WHERE {
  INCLUDE %licenses
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""" .
