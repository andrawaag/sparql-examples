<https://www.wikidata.org/#query-e30db6130fbfa7ab2a3763750e43b97b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata_talk:Events/GLAM_%26_Wikimedia_CH>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d826522-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#recherche un élément portant le no d'un identifiant externe pour lequel la propriété existe dans Wikidata
SELECT ?item ?itemLabel ?itemDescription
WHERE
{
   ?item wdt:P268 ?id. #propriété correspondant à l'identifiant d'autorité dans le catalogue de la Bibliothèque nationale de France
  FILTER ( STR(?id) = '123735831') #no recherché
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""" .
