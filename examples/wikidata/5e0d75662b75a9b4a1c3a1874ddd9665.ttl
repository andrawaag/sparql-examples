<https://www.wikidata.org/#query-5e0d75662b75a9b4a1c3a1874ddd9665> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """SELECT ?uni ?uniLabel ?loc ?link ?layer ?countyLabel WHERE {
VALUES ?type {wd:Q875538 wd:Q902104 wd:Q3918}
?uni wdt:P31 ?type;
     wdt:P17 wd:Q145;
     wdt:P856 ?link;
     wdt:P625 ?loc.
?type rdfs:label ?layer FILTER(Lang(?layer)='en').
MINUS {?uni wdt:P576 []}
OPTIONAL {?uni wdt:P131 ?county} 
SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
}""";
  rdfs:comment "Properties: instance of (P31)   , country (P17)   , official website (P856)   , coordinate location (P625)   , dissolved, abolished or demolished date (P576)   , located in the administrative territorial entity (P131)   "@en;
