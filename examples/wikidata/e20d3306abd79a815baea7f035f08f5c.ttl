<https://www.wikidata.org/#query-e20d3306abd79a815baea7f035f08f5c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Jacksonj04/prompts/South_Africa/Assembly/Peoples_Assembly/Current>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d846259-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d846259-AF8D9439AA301A342E57BAB84508F28A;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
SELECT DISTINCT ?identifier__wikidata ?name ?source ?term (REPLACE(STR(?start),\"T00:00:00Z\",\"\") AS ?start_date) WHERE {
  ?identifier__wikidata p:P39 ?statement.
  ?statement ps:P39 wd:Q16744266; pq:P580 ?start .
  OPTIONAL { ?statement pq:P582 ?end }
  OPTIONAL {
    ?statement pq:P2937 ?termStatement .
    OPTIONAL { ?termStatement p:P31/pq:P1545 ?term . }
  }
  OPTIONAL {
    ?identifier__wikidata p:P973 ?describedAtPA .
    ?describedAtPA ps:P973 ?source .
    OPTIONAL {
      ?describedAtPA pq:P1810 ?name .
    }
    FILTER(CONTAINS(LCASE(STR(?source)), \"pa.org.za/\"))
  }
  FILTER(!BOUND(?end) || ?end > NOW())
}""" .
