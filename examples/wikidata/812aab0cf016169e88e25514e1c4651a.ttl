  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d862201-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d862201-7EC373E88BCEBDA8C68ED62AB0AB654F;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title:Map of aerodromes in Lithuania
#defaultView:Map
SELECT ?item ?itemLabel (SAMPLE(?coordinate_location) AS ?coord) WHERE {
  ?item (p:P31/ps:P31/(wdt:P279*)) wd:Q62447.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE], en\". }
  ?item wdt:P17 wd:Q37.
  MINUS { ?item wdt:P576 _:b2. }
  OPTIONAL { ?item wdt:P625 ?coordinate_location. }
}
GROUP BY ?item ?itemLabel""" .
