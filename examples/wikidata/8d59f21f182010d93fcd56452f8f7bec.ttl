<https://www.wikidata.org/#query-8d59f21f182010d93fcd56452f8f7bec> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Shinnin/sparql/lexeme>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d810694-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d810694-6EA8ECA3AAB44986EE87D2BFB770A5B2;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ontolex: <http://www.w3.org/ns/lemon/ontolex#>
PREFIX dct: <http://purl.org/dc/terms/>
# Genitive case ends with 'n'
SELECT ?lexeme ?word ?form ?label WHERE {
  ?lexeme a ontolex:LexicalEntry ; 
            dct:language wd:Q1412 ; 
            wikibase:lemma ?word ;
            ontolex:lexicalForm ?form .
  
  ?form ontolex:representation ?label ;
        wikibase:grammaticalFeature wd:Q146233 .
  
  FILTER NOT EXISTS {
    ?form ontolex:representation ?label ;
          wikibase:grammaticalFeature wd:Q146233 .
    FILTER(STRENDS(?label, 'n'))
  }
}""" .
