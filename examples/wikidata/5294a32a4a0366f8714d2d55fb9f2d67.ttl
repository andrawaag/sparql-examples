@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-5294a32a4a0366f8714d2d55fb9f2d67> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Vicarage>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d84547-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d84547-F14FB67420D04DF89ED766F6056E851C;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?item ?itemLabel 
  (GROUP_CONCAT(DISTINCT ?name;     SEPARATOR = \", \") AS ?names)
  (GROUP_CONCAT(DISTINCT ?lang;     SEPARATOR = \", \") AS ?langs)
  WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en-GB,en,mul\"}
  {
    SELECT DISTINCT ?item ?name ?lang WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en-GB,en,mul\"}
      ?item wdt:P31/wdt:P279* wd:Q2031121.
      ?item wdt:P1448 ?name.
      ?item rdfs:label ?name1
      BIND (lang(?name) AS ?lang)
    }
  }
} GROUP BY ?item ?itemLabel""" .
