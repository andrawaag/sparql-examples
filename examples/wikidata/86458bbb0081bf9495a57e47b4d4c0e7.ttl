<https://www.wikidata.org/#query-86458bbb0081bf9495a57e47b4d4c0e7> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Synia:typeface-index>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d848788-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT
  ?count
  ?font ?fontLabel
  (CONCAT(\"#typeface/\", SUBSTR(STR(?font), 32)) AS ?fontUrl)
  ?fontDescription

  ?example_work ?example_workLabel
  (CONCAT(\"#work/\", SUBSTR(STR(?example_work), 32)) AS ?example_workUrl)

WHERE {
  {
  SELECT
    (COUNT(?work) AS ?count)
    ?font
    (SAMPLE(?work) AS ?example_work)
  WHERE {
    ?work wdt:P2739 ?font .
  }
  GROUP BY ?font
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """SELECT
  ?count
  ?font ?fontLabel
  (CONCAT(\"#typeface/\", SUBSTR(STR(?font), 32)) AS ?fontUrl)
  ?fontDescription

  ?example_work ?example_workLabel
  (CONCAT(\"#work/\", SUBSTR(STR(?example_work), 32)) AS ?example_workUrl)
WITH {
  SELECT
    (COUNT(?work) AS ?count)
    ?font
    (SAMPLE(?work) AS ?example_work)
  WHERE {
    ?work wdt:P2739 ?font .
  }
  GROUP BY ?font
} AS %fonts
WHERE {
  INCLUDE %fonts
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""" .
