  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d846325-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d846325-6172E8E319084B280C52A99780C14D4D;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Things named after people born in cities that are part of Hungary
SELECT ?item ?itemLabel ?person ?personLabel ?coord
WHERE
{
    ?item wdt:P138 ?person .
    ?person wdt:P19 ?place .
    ?place wdt:P17 wd:Q28 .
    ?item wdt:P625 ?coord .

	SERVICE wikibase:label { 
      bd:serviceParam wikibase:language \"hu\" }
}""" .
