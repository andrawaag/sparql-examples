  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d852751-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d852751-0D1AC6AAA0241463FCF989B35185508C;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:Map
SELECT ?place ?placeLabel ?location WHERE {
  wd:Q64436 wdt:P625 ?Wloc .
  wd:Q19675 wdt:P625 ?Eloc .
  SERVICE wikibase:box {
      ?place wdt:P625 ?location .
      bd:serviceParam wikibase:cornerWest ?Wloc .
      bd:serviceParam wikibase:cornerEast ?Eloc .
    }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""" .
