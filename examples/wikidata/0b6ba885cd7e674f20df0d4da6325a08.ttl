        ?p ?rating .
  ?rating wdt:P2899 ?age; wdt:P31/wdt:P361/wdt:P17 ?country .
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"hu,en\" .
  }
} ORDER BY ?filmLabel ?countryLabel""" .
