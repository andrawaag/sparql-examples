  <http://example.org/bigdata_select> """#defaultView:Graph
SELECT ?item1 ?image ?item1Label ?item2 ?image2 ?item2Label ?edgeLabel 
WITH { SELECT ?item1 WHERE {
VALUES ?item1 {wd:Q359381 #Adnan Khashoggi
               wd:Q7398385 #Safari Club
               wd:Q953108 #SAVAK
               wd:Q806677 #BCCI
               
             
              }
