  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
#title:Count by prefix
SELECT ?pref (COUNT(?item) AS ?n)
WHERE {
  ?item wdt:P691 ?nkc .
  BIND(REPLACE(STR(?nkc), \"^([a-z]*)\\\\d+$\", \"$1\") AS ?pref)
}
GROUP BY ?pref
ORDER BY DESC(?n)""" .
