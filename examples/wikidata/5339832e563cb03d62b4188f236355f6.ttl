<https://www.wikidata.org/#query-5339832e563cb03d62b4188f236355f6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:MisterSynergy/misc/quantity_data_type>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d838810-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
SELECT ?diff (COUNT(*) AS ?cnt)  WHERE {
  {
  SELECT ?any WHERE { ?property wikibase:propertyType wikibase:Quantity; wikibase:statementValue|wikibase:qualifierValue|wikibase:referenceValue ?any }
} .
  [] ?any [ wikibase:quantityLowerBound ?lower; wikibase:quantityUpperBound ?upper ] .
  BIND((?upper - ?lower) / 2 as ?diff) .
} GROUP BY ?diff ORDER BY DESC(?cnt)""";
  <http://example.org/bigdata_select> """SELECT ?diff (COUNT(*) AS ?cnt) WITH {
  SELECT ?any WHERE { ?property wikibase:propertyType wikibase:Quantity; wikibase:statementValue|wikibase:qualifierValue|wikibase:referenceValue ?any }
} AS %subquery WHERE {
  INCLUDE %subquery .
  [] ?any [ wikibase:quantityLowerBound ?lower; wikibase:quantityUpperBound ?upper ] .
  BIND((?upper - ?lower) / 2 as ?diff) .
} GROUP BY ?diff ORDER BY DESC(?cnt)""" .
