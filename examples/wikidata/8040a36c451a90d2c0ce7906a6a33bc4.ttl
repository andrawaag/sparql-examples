    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Gruppo_Wikidata_per_Musei,_Archivi_e_Biblioteche/Controllo_di_autorit%C3%A0_per_biblioteche_Koha>;
