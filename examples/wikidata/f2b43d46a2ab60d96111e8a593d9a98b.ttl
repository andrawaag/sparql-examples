  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Theklan>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d85495-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d85495-FEE8A368DBE82520CD80B981F0E4132D;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
SELECT ?year (COUNT(?item) AS ?count)
