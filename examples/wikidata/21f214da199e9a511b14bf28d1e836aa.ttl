<https://www.wikidata.org/#query-21f214da199e9a511b14bf28d1e836aa> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2022/07>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d841636-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?itemLabel ?itemDescription

WHERE
{
  {
  SELECT ?item
  WHERE
  {
    ?item wdt:P31 wd:Q11424 .
    MINUS
    {
      ?article schema:about ?item .
      ?article schema:isPartOf / wikibase:wikiGroup \"wikipedia\" .
    }
  }
  LIMIT 100
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
}""";
  <http://example.org/bigdata_select> """SELECT ?item ?itemLabel ?itemDescription
WITH
{
  SELECT ?item
  WHERE
  {
    ?item wdt:P31 wd:Q11424 .
    MINUS
    {
      ?article schema:about ?item .
      ?article schema:isPartOf / wikibase:wikiGroup \"wikipedia\" .
    }
  }
  LIMIT 100
} AS %items
WHERE
{
  INCLUDE %items
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }
}""" .
