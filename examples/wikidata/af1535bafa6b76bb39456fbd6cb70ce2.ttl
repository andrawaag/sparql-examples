  ?maker wdt:P6379 ?museum .        #has works in collection 
  ?museum wdt:P625 ?geocoordinates  #geocoordinates
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""" .
