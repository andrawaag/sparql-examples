  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d82999-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d82999-ED7E10C479FB0EA1034FCB3D640F963A;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# People born in Armenia that lack a statement about which languages they speak
SELECT ?item ?itemLabel WHERE {
  ?item wdt:P19 ?place .
  ?place wdt:P17 wd:Q399 .
  FILTER NOT EXISTS { ?item wdt:P1412 ?language }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""" .
