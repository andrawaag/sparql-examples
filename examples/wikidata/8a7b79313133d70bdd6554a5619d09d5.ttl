          ?item wdt:P31 wd:Q5.
          ?item wdt:P106 wd:Q2309784.
          optional {?item wdt:P27 ?citizen.}
          } group by ?item } as %i 
where
{
  include %i
  filter(?no_of_citizenships>2)
