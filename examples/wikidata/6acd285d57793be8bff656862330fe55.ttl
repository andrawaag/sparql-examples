  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d843850-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d843850-7C740C0C006A74047633D7FEE6E07BB0;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX bned:  <https://datos.bne.es/def/> 
PREFIX bner:  <https://datos.bne.es/resource/> 

select ?llibre ?titol 
                 
          
               where {
                 {
  SELECT ?llibre ?titol ?bneid ?cosa WHERE
{
wd:Q234663 wdt:P950 ?bneid
           BIND (URI(CONCAT(\"https://datos.bne.es/resource/\",?bneid)) as ?cosa )             
  } }
SERVICE <http://datos.bne.es/sparql> {
                 
  ?bneid bned:OP5001 ?llibre.
?llibre rdfs:label ?titol
}
      }
LIMIT 2""";
  <http://example.org/bigdata_select> """PREFIX bned:  <https://datos.bne.es/def/> 
PREFIX bner:  <https://datos.bne.es/resource/> 

select ?llibre ?titol 
               WITH {
  SELECT ?llibre ?titol ?bneid ?cosa WHERE
