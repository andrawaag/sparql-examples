  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d836209-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d836209-ECA0EA9BC8C95AAC5BF0F5212FDB4FE7;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT 
    ?item ?itemLabel 
    ?whatever ?whateverLabel 
    (COUNT(DISTINCT ?sport) as ?ct)
{
    ?item wdt:P641 ?whatever .  
    ?item wdt:P641 ?sport 
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY
    ?item ?itemLabel 
    ?whatever ?whateverLabel 
HAVING (?ct > 1)""" .
