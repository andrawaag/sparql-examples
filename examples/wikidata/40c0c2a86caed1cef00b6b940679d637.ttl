<https://www.wikidata.org/#query-40c0c2a86caed1cef00b6b940679d637> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Bamyers99>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d811822-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?school ?schoolLabel (COUNT(?isni) AS ?isniC) {
  {
    SELECT DISTINCT ?school ?isni {
      ?school wdt:P31/wdt:P279* wd:Q3914 .
      ?school wdt:P213 ?isni .
      ?school wdt:P17 wd:Q30 .
    }
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
GROUP BY ?school ?schoolLabel
HAVING (COUNT(?isni) > 1)
ORDER BY ?schoolLabel""" .
