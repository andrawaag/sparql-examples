  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d820522-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d820522-5A205D603F6A26BC052FF7E1020DF7C8;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ontolex: <http://www.w3.org/ns/lemon/ontolex#>
PREFIX dct: <http://purl.org/dc/terms/>
# Inessive case ends with 'ssA'
SELECT ?lexeme ?word ?form ?label WHERE {
  ?lexeme a ontolex:LexicalEntry ; 
            dct:language wd:Q1412 ; 
            wikibase:lemma ?word ;
            ontolex:lexicalForm ?form .
  
  ?form ontolex:representation ?label ;
        wikibase:grammaticalFeature wd:Q282031 .
  
  FILTER NOT EXISTS {
    ?form ontolex:representation ?label ;
          wikibase:grammaticalFeature wd:Q282031 .
    FILTER(STRENDS(?label, 'ssa')|| STRENDS(?label, 'ss√§'))
  }
}""" .
