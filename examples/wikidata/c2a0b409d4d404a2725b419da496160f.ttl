  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d841285-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d841285-0B0E8E6040B223D4045F7E43A34F4C90;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# List all items that have a type locality
select ?item ?itemLabel ?imaLabel ?locLabel where
