  ?rank wdt:P31/wdt:P279* wd:Q4226220 .
  # occupation is a military rank
  # ?rank wdt:P31/wdt:P279* wd:Q56019 .

  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"en\" .
