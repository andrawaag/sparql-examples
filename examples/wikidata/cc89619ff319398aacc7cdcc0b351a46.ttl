<https://www.wikidata.org/#query-cc89619ff319398aacc7cdcc0b351a46> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Property_talk:P2860>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d846670-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:BubbleChart
SELECT ?author ?authorLabel (COUNT(?publication) AS ?count)
WHERE
{
    ?item wdt:P2860 ?publication .
    ?publication wdt:P50 ?author .
    SERVICE wikibase:label {
        bd:serviceParam wikibase:language \"en\" .
    }
}
GROUP BY ?author ?authorLabel
ORDER BY DESC(?count)""" .
