  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d816043-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d816043-71B13C8ED62360BFDBCA77F3B7EFB1AA;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#Scrittori di un genere con SHARE Cat ID e senza SHARE-VDE ID
SELECT ?item ?itemLabel ?shareID
WHERE
{
  ?item wdt:P3987 ?shareID; 
        wdt:P106 wd:Q36180;
        wdt:P136 wd:Q24925.
  OPTIONAL {?item wdt:P6329 ?shareVDE.}
  FILTER(!bound(?shareVDE))
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  }
ORDER BY ?item""" .
