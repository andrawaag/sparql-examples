<https://www.wikidata.org/#query-fa25ab8a8a9d264b44a22cfa246b440f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Uomovariabile/Queries/SHARE_Catalogue>;
