  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d862159-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:Map
SELECT ?item ?itemLabel ?coord WHERE {
  ?item wdt:P17 wd:Q1460. # Autonome Region Sizilien
  ?item wdt:P625 ?coord.
  ?item wdt:P31/wdt:P279* wd:Q23397. # See oder Unterklasse
  MINUS { ?item wdt:P131 [] } # ohne administrative Lage
SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,it,en,[AUTO_LANGUAGE]\" . }
}""" .
