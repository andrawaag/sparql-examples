<https://www.wikidata.org/#query-33f19958914965d82c8c082b1d7c77d2> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Template:GenderPercent/doc>, <https://www.wikidata.org//wiki/Template:GenderPercent>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d83167-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d83167-255B149A368D0048EA4567BF05220E8F;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
select (?countitemg/?countitemt*100 as ?item_percent) ?item_gender {
  { 
    select (count(?item) as ?countitemg ) ?item_gender where { 
       ?item wdt:P31 wd:Q5 . ?item wdt:P27 wd:Q142 . # french people
       ?item wdt:P21 ?item_gender filter (!isBlank(?item_gender)) .
    } group by ?item_gender
  } 
  {
    select (count(?item) as ?countitemt) where {      
       ?item wdt:P31 wd:Q5 . ?item wdt:P27 wd:Q142 . # french people
    }
  }
}""" .
