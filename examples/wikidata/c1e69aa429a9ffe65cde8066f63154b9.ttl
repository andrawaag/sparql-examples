<https://www.wikidata.org/#query-c1e69aa429a9ffe65cde8066f63154b9> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Uomovariabile/Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d860404-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d860404-05104D5751CCAD09D347321DE18865A0;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?softwareLabel ?developerLabel ?yearofbirth ?employerLabel

WHERE {
  
  ?software wdt:P31 wd:Q341 .
  ?software wdt:P178 ?developer .
  ?developer wdt:P569 ?dateofbirth .
  OPTIONAL {?developer wdt:P108 ?employer .}
  BIND(YEAR(?dateofbirth) AS ?yearofbirth).
  
  SERVICE wikibase:label {
		bd:serviceParam wikibase:language \"en\" .
    
}
  }
ORDER BY ?yearofbirth""" .
