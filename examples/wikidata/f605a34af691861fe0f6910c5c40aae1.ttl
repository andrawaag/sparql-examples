SELECT ?item ?itemLabel ?ruWP ?enWS ?ruWS
WHERE 
{
# ?item wdt:P31/wdt:P279* wd:Q3331189. #edition
  ?item wdt:P31/wdt:P279* wd:Q7725634. #literary work
        
