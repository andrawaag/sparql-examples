<https://www.wikidata.org/#query-6934eefc94ddce85c8c66e45d18cc489> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Zygimantus/Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d820717-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d820717-5ACEF2909A9FBB8BCECF6A8EFE9F490F;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title:Women scientist (loose meaning) in other language Wikipedias, but not in the Lithuanian Wikipedia
SELECT ?item ?itemLabel ?linkcount WHERE {
  ?item wdt:P31 wd:Q5;
    wdt:P21 wd:Q6581072;
    wdt:P106 ?occupation;
    wikibase:sitelinks ?linkcount.
  FILTER(?linkcount > 14 )
  MINUS {
    ?sitelinklt schema:isPartOf <https://lt.wikipedia.org/>;
      schema:about ?item.
  }
  ?occupation (wdt:P279*) wd:Q901.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?item ?itemLabel ?linkcount
ORDER BY DESC (?linkcount)""" .
