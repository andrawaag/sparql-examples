<https://www.wikidata.org/#query-8b8c980b8ed59a1f51d7928857b0d5a3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:GranD>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d827947-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?surname ?surnameLabel (COUNT(?item) AS ?count)
WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"ru,en\". }
  ?item wdt:P31 wd:Q5.
  ?item wdt:P734 ?surname.
#  ?item wdt:P735 ?surname. # имя
  ?surname wdt:P31 wd:Q4167410
}
GROUP BY ?surname ?surnameLabel
ORDER BY DESC(?count)
LIMIT 100""" .
