<https://www.wikidata.org/#query-24ca436d6c91d3bb8f7bf7e6bfc55277> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Jobu0101/API>;
