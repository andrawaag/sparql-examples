<https://www.wikidata.org/#query-93f3538b6207b1852d394455b6500362> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:MartinPoulter/queries/screennation>;
