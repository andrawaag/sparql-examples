  OPTIONAL { ?lexeme1 ontolex:sense / wdt:P5137? / wdt:P18 ?lexeme1Image }
  OPTIONAL { ?lexeme2 ontolex:sense / wdt:P5137? / wdt:P18 ?lexeme2Image }
