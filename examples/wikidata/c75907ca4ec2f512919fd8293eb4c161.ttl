<https://www.wikidata.org/#query-c75907ca4ec2f512919fd8293eb4c161> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:PAC2/Ministres>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d859066-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?itemLabel (SUM (?duree_mandat) AS ?duree) WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr\". }
    ?item p:P39 ?statement.
    ?statement ps:P39 ?ministere.
    ?ministere (wdt:P279*) wd:Q1587677 ;
      wdt:P17 wd:Q142.
   ?statement pq:P580 ?datedebut.
   ?statement pq:P582 ?datefin.
   BIND( (?datefin - ?datedebut) AS ?duree_mandat )
  FILTER(YEAR(?datedebut) >= 1945).
}
GROUP BY ?item ?itemLabel
ORDER BY DESC (?duree)""" .
