SELECT DISTINCT ?item ?itemLabel ?instanceOfLabel ?partLabel ?locationLabel WHERE {
{
  ?item wdt:P361 wd:Q812285 . # parts of BStGS
} UNION {
  ?item wdt:P361 ?sub1 . # parts of
