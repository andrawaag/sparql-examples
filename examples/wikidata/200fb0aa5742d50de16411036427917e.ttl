  BIND (STRLANG(?title, \"it\") AS ?title_it)
  ?article_it schema:name ?title_it .
  ?article_it schema:about ?item .
  ?article_it schema:isPartOf <https://it.wikipedia.org/> .
  ?item wdt:P31 wd:Q5 .
  MINUS { ?item wdt:P106 [] } 
}
