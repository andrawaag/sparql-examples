#place=*
SELECT DISTINCT ?osmId ?loc ?wda ?que ?queLabel WHERE {
  ?osmId osmt:historic 'optical_telegraph'.
  ?osmId osmm:loc ?loc .
  ?osmId osmt:wikidata ?wda . 
