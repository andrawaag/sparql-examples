<https://www.wikidata.org/#query-8bd50856a37a8690561e6db8e630977c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Talk:Q115664643>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d847033-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d847033-73E5B3DD51FE31F581B3AAA17B7B2FFB;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX dct: <http://purl.org/dc/terms/>
SELECT ?lexemeId ?lemma ?dicoentry ?refnumber ?startpage ?endpage WHERE {
  ?lexemeId dct:language wd:Q150 ; wikibase:lemma ?lemma ; p:P1343 ?dico.
  ?dico ps:P1343 wd:Q115664643 ; pq:P304 ?page ; pq:P958 ?ref ; pq:P1932 ?dicoentry.
  BIND(xsd:integer(?ref) AS ?refnumber)
  BIND(xsd:integer(REPLACE(?page, \"-[0-9]*\", \"\")) AS ?startpage)
  BIND(xsd:integer(REPLACE(?page, \"[0-9]*-\", \"\")) AS ?endpage) 
}
ORDER BY ASC(?refnumber)""" .
