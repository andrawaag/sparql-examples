<https://www.wikidata.org/#query-1d104c735986b62dcccbaadf817b8cf2> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Methodios/Dresden>;
