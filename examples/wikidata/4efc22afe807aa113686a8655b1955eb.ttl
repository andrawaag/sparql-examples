  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d838336-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d838336-2E4B0011A31BA1C9199747E59C6E86E7;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#For book properties
#defaultView:BubbleChart
SELECT ?property ?propertyLabel ?propertyDescription ?count WHERE { 
	{
		select ?propertyclaim (COUNT(*) AS ?count) where {
			?item wdt:P31 wd:Q571 .
			?item ?propertyclaim [] .
		} group by ?propertyclaim 
	}
	?property wikibase:claim ?propertyclaim .
	SERVICE wikibase:label { bd:serviceParam wikibase:language \"fr,en\" . }
} ORDER BY DESC (?count)""" .
