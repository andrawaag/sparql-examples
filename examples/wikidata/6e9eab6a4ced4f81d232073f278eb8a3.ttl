<https://www.wikidata.org/#query-6e9eab6a4ced4f81d232073f278eb8a3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Carlobia/CCLitBoxPresentazione>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d820702-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Elenco di romanzieri italiani nati nel 900 in ordine di data di nascita
SELECT ?item ?itemLabel ?ddn ?cc WHERE {
  
  ?item wdt:P31 wd:Q5 .
  ?item wdt:P106 wd:Q6625963 .
  ?item wdt:P569 ?ddn .
  ?item wdt:P27 wd:Q38 .
  OPTIONAL { ?item wdt:P8248 ?cc . }

  FILTER((?ddn >= \"1900-01-01T00:00:00Z\"^^xsd:dateTime) && (?ddn<=\"1999-12-31T00:00:00Z\"^^xsd:dateTime))
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY ?ddn""" .
