  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d855745-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d855745-23E7FB2BF9A4B857BD9B4312B27D9069;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Русские имена на латинице
SELECT DISTINCT ?name ?nameLabel (COUNT(DISTINCT ?item) AS ?cnt) WHERE {
  ?name wdt:P31 wd:Q11879590.
  ?name wdt:P407 wd:Q7737.
  ?name wdt:P282 wd:Q8229.
  ?item wdt:P735 ?name.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"ru\". }
}
GROUP BY ?name ?nameLabel
ORDER BY DESC(?cnt)""" .
