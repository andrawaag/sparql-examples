<https://www.wikidata.org/#query-54caea93d23a310c8da69154822ea34f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Husky/Zomergasten>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d846673-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
select (avg(?age) as ?avgAge) where {
  ?item wdt:P179 wd:Q2330424;
        wdt:P5030 ?guest;
        wdt:P577 ?pubDate. 
  ?guest wdt:P569 ?dob. 
  bind (year(?dob) as ?dobyear). 
  bind (year(?pubDate) as ?pubyear).
  bind (?pubyear - ?dobyear as ?age).
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"nl\". }
}""" .
