<https://www.wikidata.org/#query-76ba04f4815e8e79a93e300d96082c96> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Hungary/Showcase_Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d838279-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d838279-FEEF9972F66296E6F7A18532FD18DC0D;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Who are things in Hungary named after in descending order
SELECT ?nameSake ?nameSakeLabel ?count
WHERE
{
  {
    SELECT ?nameSake (COUNT(?item) AS ?count)
    WHERE
    {
        {
          ?item wdt:P131* wd:Q28 .
        }
        UNION
        {
          ?item wdt:P17 wd:Q28 .
        }
        ?item wdt:P138 ?nameSake .
    }
    GROUP BY ?nameSake
  }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"hu,en\" }
}
ORDER BY DESC (?count)""" .
