  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d840901-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d840901-A4D3FFA3F72C5342A5811C0BCCD864D5;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# illustrates sitelink selection, \";\" notation
#title: Items with a Wikispecies sitelink
SELECT ?item ?itemLabel ?article

WHERE {
  {
  SELECT *
  WHERE {
    ?article schema:about ?item ;
      schema:isPartOf <https://species.wikimedia.org/> .
  }
  LIMIT 200
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],mul,en\" . }
}""";
  <http://example.org/bigdata_select> """# illustrates sitelink selection, \";\" notation
#title: Items with a Wikispecies sitelink
SELECT ?item ?itemLabel ?article
WITH {
  SELECT *
  WHERE {
    ?article schema:about ?item ;
      schema:isPartOf <https://species.wikimedia.org/> .
  }
  LIMIT 200
} AS %i
WHERE {
  INCLUDE %i
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],mul,en\" . }
}""" .
