<https://www.wikidata.org/#query-447d259f5128462a2afdfa837a3e130b> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Ren%C3%A9LC>;
