  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d86281-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
PREFIX target: <http://www.wikidata.org/entity/Q308915>

SELECT
  ?count
  ?class ?classLabel
  ?classDescription
 
  ?example_work ?example_workLabel
  (CONCAT(\"#work/\", SUBSTR(STR(?example_work), 32)) AS ?example_workUrl)

WHERE {
  {
  SELECT 
    (COUNT(*) AS ?count)
    ?class
    (SAMPLE(?work) AS ?example_work)
  WHERE {
    ?work wdt:P31 ?class ;
          wdt:P275 target: .
  } 
  GROUP BY ?class
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """PREFIX target: <http://www.wikidata.org/entity/Q308915>

SELECT
  ?count
  ?class ?classLabel
  ?classDescription
 
  ?example_work ?example_workLabel
  (CONCAT(\"#work/\", SUBSTR(STR(?example_work), 32)) AS ?example_workUrl)
WITH {
  SELECT 
    (COUNT(*) AS ?count)
    ?class
    (SAMPLE(?work) AS ?example_work)
  WHERE {
    ?work wdt:P31 ?class ;
          wdt:P275 target: .
  } 
  GROUP BY ?class
} AS %classes
WHERE {
  INCLUDE %classes
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""" .
