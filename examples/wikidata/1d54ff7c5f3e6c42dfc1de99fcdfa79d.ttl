  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d815206-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d815206-9B005E4A554C4A2F22F0C23931F79FDC;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
# Query to count burials recorded for Père-Lachaise cemetery by year
SELECT ?year (COUNT (DISTINCT ?a) AS ?count) WHERE {
   ?a wdt:P119 wd:Q311 .    # buried at Père-Lachaise cemetery
   ?a wdt:P570 ?date .
   BIND(year(?date) AS ?year)
} GROUP BY ?year
ORDER BY ?year""" .
