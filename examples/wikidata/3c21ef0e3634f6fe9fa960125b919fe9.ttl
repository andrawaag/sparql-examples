<https://www.wikidata.org/#query-3c21ef0e3634f6fe9fa960125b919fe9> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Nurni/001>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d833680-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT DISTINCT ?item ?typ
{
    ?item wdt:P31 wd:Q3305213.
    ?item wdt:P18 ?img.
    OPTIONAL {?item wdt:P136 ?genre.}.
    FILTER (!BOUND(?genre)).
  {
    ?item rdfs:label ?nazwa.
    FILTER(REGEX(?nazwa, CONCAT(\"^.*(self[- ]?portrait|autoretrat|autorretrato|autoritratto|selbstporträt|selbstbildnis|autoportrait|\",
                                \"автопортрет|zelf|självporträtt|portret własny|autoportre|painter.*portrait|selvportræt).*$\"
                               ), 
                 'i')
          )
    BIND('autoportret' as ?typ)
  }
}
ORDER BY ?typ""" .
