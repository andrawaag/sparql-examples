WITH {
  SELECT ?reference (COUNT(*) AS ?count) WHERE {
    ?entity p:P569 ?statement.
    ?statement prov:wasDerivedFrom ?reference.
  }
  GROUP BY ?reference
  ORDER BY DESC(?count)
  LIMIT 100
} AS %commonReferences
WITH {
  SELECT * WHERE {
    INCLUDE %commonReferences.
    MINUS { ?reference pr:P143/wdt:P31 wd:Q10876391. }
