WHERE {
  INCLUDE %commonRealReferences.
  ?reference ?pr ?value.
  ?property wikibase:reference ?pr.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count) ASC(?propertyLabel)
