<https://www.wikidata.org/#query-11a810e73c661344521633bbdc82dbfc> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>

SELECT ?item ?DWBID ?arkbnf ?ppn WHERE {
  ?item wdt:P1648 ?DWBID ; wdt:P31 wd:Q5.
  OPTIONAL {?item wdt:P268 ?arkbnf}
  OPTIONAL {?item wdt:P269 ?ppn}
  FILTER(bound(?arkbnf)|| bound(?ppn))
}""";
