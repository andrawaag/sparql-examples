  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d860980-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?series ?seriesLabel  ((?anzahl-COUNT(?episode)) AS ?count) WHERE {
  ?episode wdt:P31 wd:Q21191270 .
  ?episode wdt:P345 ?imdb .
  ?episode wdt:P179 ?series .
  ?series wdt:P1113 ?anzahl
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
} GROUP BY ?series ?seriesLabel ?anzahl ORDER BY DESC(?count)""" .
