  ?article schema:about ?item.
  ?item wdt:P31 wd:Q5.
  ?article schema:name ?title.
  ?article schema:isPartOf ?Wikipedia.
  ?Wikipedia wikibase:wikiGroup \"wikipedia\".
  FILTER CONTAINS(?title, \"Vale\")
}
LIMIT 100""" .
