  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d841687-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?chiesa ?chiesaLabel ?coordinate_geografiche WITH { SELECT ?chiesa WHERE {
  ?chiesa wdt:P131* wd:Q16289.
  } } as %i
WHERE
{
  INCLUDE %i         
  ?chiesa wdt:P31/wdt:P279* wd:Q16970.
  FILTER NOT EXISTS { ?chiesa wdt:P625 ?coordinate_geografiche. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"it,en\". }
}""";
