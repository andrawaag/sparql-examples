<https://www.wikidata.org/#query-5217646cab4b93364fb616877db9e06a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Latvia/Maintenance_Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d845014-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d845014-3C1F9B29BDA0DF52057E12951EB42CC0;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?occup ?occupLabel ?count
WHERE
{
	{
	SELECT ?occup (COUNT(?person) as ?count)
	WHERE
	{
		?person wdt:P106 ?occup .
	}
	GROUP BY ?occup
	ORDER BY DESC(?count)
	LIMIT 1000
	}
  	OPTIONAL {?occup rdfs:label ?name_label filter(lang(?name_label) = \"lv\")}
	FILTER(!BOUND(?name_label))
  
    SERVICE wikibase:label {
        bd:serviceParam wikibase:language \"en\" .
    }
}
ORDER BY DESC(?count)""" .
