  OPTIONAL { ?item wdt:P569 ?date_of_birth }
  OPTIONAL { ?item wdt:P570 ?date_of_death }
  OPTIONAL { ?item wdt:P18 ?image }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"nl,en,de,fr\" }
}""" .
