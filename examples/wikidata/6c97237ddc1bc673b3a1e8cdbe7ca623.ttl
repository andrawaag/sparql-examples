<https://www.wikidata.org/#query-6c97237ddc1bc673b3a1e8cdbe7ca623> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Bangbang.S>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d81508-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:Map
SELECT DISTINCT ?item ?itemLabel ?deathNumber ?aircraftLabel ?geo ?date ?layer WHERE {
  ?item (wdt:P31/(wdt:P279*)) wd:Q744913.
  OPTIONAL {
    ?item wdt:P121 ?aircraft;
      wdt:P585 ?date;
      wdt:P625 ?geo;
      wdt:P1120 ?deathNumber.
  }
  BIND (
    IF(?deathNumber <=1, \"<=1\",
    IF(?deathNumber <=5, \"1,5\",
    IF(?deathNumber <=10, \"5,10\",
    IF(?deathNumber <=50, \"10,50\",
    \">=50\"))))
    AS ?layer)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],zh\". }
}""" .
