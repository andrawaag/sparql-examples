  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d852118-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d852118-F3D1033213727EACA69DDAEA250369CE;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
#defaultView:Map
SELECT ?item ?itemLabel ?coords WHERE {
  ?item wdt:P31 / wdt:P279* wd:Q515;
        wdt:P17 wd:Q159;
        wdt:P625 ?coords;
        rdfs:label ?itemLabel FILTER( LANG( ?itemLabel ) = 'en' ) .
  FILTER ( strEnds( ?itemLabel, 'pol' ) ) .
}""" .
