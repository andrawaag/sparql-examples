<https://www.wikidata.org/#query-50bdcf1d53ff51d671ebd15e88602534> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Zygimantus/Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d848731-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d848731-82972E8D761F3BC0BAB4E862CEAAADDA;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Sample to query all values of a property
# Property talk pages on Wikidata include basic queries adapted to each property
SELECT
  ?item ?itemLabel
  ?value ?valueLabel
  ?whatLabel
# valueLabel is only useful for properties with item-datatype
WHERE
{
  ?item wdt:P10040 ?value .
  ?item wdt:P31 ?what
  # change P10040 to another property
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
# remove or change limit for more results
LIMIT 10""" .
