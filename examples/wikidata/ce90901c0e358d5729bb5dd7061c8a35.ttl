  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d81721-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#NARA P1225 repartition of the s√©ries by part of elements
SELECT ?partie ?partieLabel (COUNT(?item) as ?total)
WHERE 
{
  ?item wdt:P1225 ?ID_NARA;
        wdt:P31 wd:Q2668072;
        wdt:P6224 wd:Q3511132;
        wdt:P361 ?partie.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?partie ?partieLabel
ORDER BY DESC(?total)""" .
