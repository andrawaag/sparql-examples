<https://www.wikidata.org/#query-0cc758504599c7a1779a4b8697dc1afa> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Progetto_Partigiani_Toscani>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d837082-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title:Partigiani morti in Toscana
SELECT ?partigiano ?partigianoLabel ?partigianoDescription ?luogonascitaLabel ?luogomorteLabel ?ANPI
WHERE {
  ?partigiano wdt:P106 wd:Q212948 .
  OPTIONAL { ?partigiano wdt:P19 ?luogonascita } .
  ?partigiano wdt:P20 ?luogomorte . ?luogomorte wdt:P131* wd:Q1273 .
  OPTIONAL { ?partigiano wdt:P9267 ?ANPI } .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY xsd:integer(?ANPI)""" .
