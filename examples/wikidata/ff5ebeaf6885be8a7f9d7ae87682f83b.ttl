  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d855571-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d855571-C58C7718D9DDBC82A32C32EF9883978E;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?place ?placeLabel ?location WHERE
{
  wd:Q64436 wdt:P625 ?arcLoc .
  SERVICE wikibase:around {
      ?place wdt:P625 ?location .
      bd:serviceParam wikibase:center ?arcLoc .
      bd:serviceParam wikibase:radius \"1\" .
  }
  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"en\" .
  }
}""" .
