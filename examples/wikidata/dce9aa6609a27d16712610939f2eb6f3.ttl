<https://www.wikidata.org/#query-dce9aa6609a27d16712610939f2eb6f3> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:SAPA_bdc/SPARQL>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d858949-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Liste des productions des arts de la scène (Q43099500) (ou l'une de ses sous-classes) dont l'une des société de production est suisse (sauf Schauspielhaus ZH).
SELECT ?item ?itemLabel ?periodeLabel ?production ?productionLabel
WHERE 
{
  ?item wdt:P31/wdt:P279* ?nature;
        wdt:P272 ?production.
  VALUES ?nature {wd:Q43099500}.#qui est une production des arts de la scène
  ?production wdt:P17 wd:Q39.#la société de production est située en Suisse.
  VALUES ?minus {wd:Q40313234}.
  MINUS {?item wdt:P272 ?minus}.#la société de production n'est pas le Schauspielhaus ZH
  MINUS {?item wdt:P31 ?nature2}.
  VALUES ?nature2 {wd:Q182832}.#qui n'est pas un concert 
  OPTIONAL {?item wdt:P2348 ?periode}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],de,it,en\". }
}""" .
