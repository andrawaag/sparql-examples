  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d829729-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d829729-425DC13E087422DB901F73C37701B0B2;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>

SELECT ?sailboat ?sailboatLabel ?desc ?property ?qualifierLabel
WHERE 
{
  ?sailboat wdt:P31 wd:Q106179098 .
  ?sailboat wdt:P2043 ?property .
  ?sailboat schema:description ?desc .
  ?sailboat p:P2043 [ps:P2043 ?property;
                     pq:P642 ?qualifier;
                     pq:P642 wd:Q1817392;
                    ].
  
  FILTER ( lang(?desc) = \"en\" )
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""" .
