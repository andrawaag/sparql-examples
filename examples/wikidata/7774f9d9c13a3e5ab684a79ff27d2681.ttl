  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d834175-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d834175-8DD731AC16B4EDA532E35917B68028A9;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
SELECT DISTINCT ?city ?country WHERE {
  ?city wdt:P31/wdt:P279* wd:Q515 ; wdt:P1082 ?population .
  FILTER (?population > 1000000) .
  FILTER NOT EXISTS { ?city wdt:P576 [] } .
  OPTIONAL { ?city wdt:P17 ?country }
} 
ORDER BY DESC(?population)""" .
