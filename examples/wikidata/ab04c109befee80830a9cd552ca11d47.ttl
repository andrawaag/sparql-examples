  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d810904-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d810904-1FF5F476151BE38D9B41614348EECE70;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?item ?itemLabel 
WHERE
{
  ?item wdt:P31 wd:Q3745054 . 
  OPTIONAL { 
    ?sitelink schema:about ?item ; 
              schema:inLanguage \"fr\" .
  } 
  FILTER (!BOUND(?sitelink))

  SERVICE wikibase:label {
    bd:serviceParam wikibase:language \"fr,en,de\" .
  }
}
ORDER BY ?itemLabel""" .
