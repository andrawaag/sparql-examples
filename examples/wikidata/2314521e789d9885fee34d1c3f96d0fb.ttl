    _:genid-4e694113159d4e3db4a1a913894a81d835669-2A8BB082FA89D7BFE19560165B157769, _:genid-4e694113159d4e3db4a1a913894a81d835669-F8D10E7686C25724F56BB1D43A7DDF3E;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
#defaultView:Timeline
SELECT
  ?item
  ?itemLabel
  ?article
  ?topics
  ?st
  ?et
{
  {
    SELECT
      ?item
      (GROUP_CONCAT(DISTINCT ?msLabel; SEPARATOR = \", \") AS ?topics)
    {
      ?item wdt:P31 wd:Q16022392;
            wdt:P17 wd:Q419;
            wdt:P921 ?ms.
      ?ms rdfs:label ?msLabel;
          FILTER(LANG(?msLabel) = \"es\").
    }
    GROUP BY ?item
  }
  {
    SELECT
      ?item
      ?itemLabel
      ?article
      ?st
      ?et
    {
      ?item wdt:P31 wd:Q16022392;
            wdt:P580 ?st;
            wdt:P582 ?et;
            rdfs:label ?itemLabel;
            FILTER(LANG(?itemLabel) = \"es\").
      ?article schema:about ?item.
    }
  }
}""" .
