    ?item wdt:P31 wd:Q327333 .
    ?item wdt:P361 ?ministry .
    ?ministry wdt:P31 wd:Q127437239 .
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
} ORDER BY ?itemLabel""" .
