#defaultView:Map
SELECT DISTINCT ?item ?itemLabel ?coords ?image WHERE {
      ?item wdt:P31/wdt:P279? wd:Q16917. # Hospital or type of hospital
      ?item wdt:P131/wdt:P131? wd:Q23169. # In Oxfordshire or a (part of)* Oxfordshire
