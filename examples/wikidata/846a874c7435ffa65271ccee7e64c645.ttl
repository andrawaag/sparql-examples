<https://www.wikidata.org/#query-846a874c7435ffa65271ccee7e64c645> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Synia:publisher>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d830443-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
PREFIX target: <http://www.wikidata.org/entity/Q7894897>

SELECT 
  ?count
  (CONCAT(\"#publisher/\", SUBSTR(STR(target:), 32),
          \"/topic/\", SUBSTR(STR(?topic), 32)) AS ?countUrl)

  ?topic ?topicLabel
  (CONCAT(\"#topic/\", SUBSTR(STR(?topic), 32)) AS ?topicUrl)
  ?topicDescription

  ?example_work ?example_workLabel
  (CONCAT(\"#work/\", SUBSTR(STR(?example_work), 32)) AS ?example_workUrl)

WHERE {
  {
  SELECT 
    (COUNT(DISTINCT ?work) AS ?count)
    ?topic
    (SAMPLE(?work) AS ?example_work)
  WHERE {
    ?work wdt:P123 target: ;
          wdt:P921 ?topic .
  }
  GROUP BY ?topic
  ORDER BY DESC(?count)
  LIMIT 200
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """PREFIX target: <http://www.wikidata.org/entity/Q7894897>

SELECT 
  ?count
  (CONCAT(\"#publisher/\", SUBSTR(STR(target:), 32),
          \"/topic/\", SUBSTR(STR(?topic), 32)) AS ?countUrl)

  ?topic ?topicLabel
  (CONCAT(\"#topic/\", SUBSTR(STR(?topic), 32)) AS ?topicUrl)
  ?topicDescription

  ?example_work ?example_workLabel
  (CONCAT(\"#work/\", SUBSTR(STR(?example_work), 32)) AS ?example_workUrl)
WITH {
  SELECT 
    (COUNT(DISTINCT ?work) AS ?count)
    ?topic
    (SAMPLE(?work) AS ?example_work)
  WHERE {
    ?work wdt:P123 target: ;
          wdt:P921 ?topic .
  }
  GROUP BY ?topic
  ORDER BY DESC(?count)
  LIMIT 200
} AS %topics
WHERE {
  INCLUDE %topics
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""" .
