SELECT ?link ?itemLabel ?image ?date ?datePrecision ?placeLabel ?collectionLabel WHERE {
  VALUES ?collection {wd:Q66455355 wd:Q66458773}
  ?item p:P195 [ps:P195 ?collection; pq:P217 ?inv ] FILTER(STRSTARTS(?inv, \"MTW\"))
