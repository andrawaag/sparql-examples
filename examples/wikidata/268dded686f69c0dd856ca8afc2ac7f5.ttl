<https://www.wikidata.org/#query-268dded686f69c0dd856ca8afc2ac7f5> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2018/01>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d828796-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT  ?object ?objectLabel (YEAR(?date) as ?year)
WHERE
{
  ?object wdt:P31 wd:Q577.        #item is a year 
  ?object wdt:P585 ?date .        #item has a 'point in time' property
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }               
  FILTER (YEAR(?date) > 2100)     #value from the 'point in time' property greater than 2100
}
ORDER BY ?year""" .
