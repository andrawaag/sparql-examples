  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d817438-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d817438-D8F4500D045FAAC454DE06308C95977B;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title:Architects ordered by number of projects in Vilnius
#defaultView:BubbleChart
SELECT ?archLabel (COUNT(DISTINCT ?city) AS ?count) WHERE {
  ?city (wdt:P131/(wdt:P131*)) wd:Q216;
    wdt:P84 ?arch.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?arch ?archLabel
ORDER BY DESC (?count)""" .
