  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d847231-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d847231-606E9A68AF29A1FCC0760BC9E76C49AC;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?itemLabel ?idSHARE ?sitelink
WHERE
{
  ?item wdt:P106 wd:Q170790 .
  ?item wdt:P3987 ?idSHARE .
  OPTIONAL {?item wdt:P214 ?viafID .}
  ?sitelink schema:about ?item . ?sitelink schema:inLanguage \"en\" 
  FILTER (!BOUND(?viafID))
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  }""" .
