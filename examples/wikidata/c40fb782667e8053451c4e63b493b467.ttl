SELECT DISTINCT ?station1 ?station1Label ?coord1 ?nowiki ?rgb ?line
WHERE {
  ?class wdt:P279* wd:Q928830 .
  ?station1 p:P31 ?st1inst_st;
    wdt:P16 wd:Q24492;
    wdt:P625 ?coord1.
  ?st1inst_st ps:P31 ?class;
    a wikibase:BestRank .
  MINUS { ?st1inst_st pq:P582 [] . }
