                wdt:P625 ?geographische_Koordinaten.
  OPTIONAL {
    ?Kulturdenkmal wdt:P18 ?Bild.
    }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\"  }
  }
ORDER BY ?KulturdenkmalLabel""" .
