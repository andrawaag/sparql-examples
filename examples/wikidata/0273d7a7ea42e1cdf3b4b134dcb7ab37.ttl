  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d857452-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d857452-EF4EE4B6BF2EFEBC410121D22E1E06D6;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
select ?subject ?subjectLabel (COUNT(?work) AS ?count) where {
  ?work wdt:P31 wd:Q13442814; 
