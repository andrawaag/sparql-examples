  FILTER(NOT EXISTS { ?museum wdt:P17 ?country. })
  OPTIONAL { ?museum wdt:P571 ?inception. }
  OPTIONAL { ?museum wdt:P131 ?located_in_the_administrative_territorial_entity. }
  OPTIONAL { ?museum wdt:P625 ?coordinate_location. }
}""" .
