<https://www.wikidata.org/#query-47cfbd4011448c07758bedbb0dec89d6> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "Properties: award received (P166)   , sex or gender (P21)   "@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:MKar/Query>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d842764-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?count ?gender ?genderLabel 

WHERE {
  {
  SELECT (COUNT(DISTINCT ?recipient) AS ?count) ?gender WHERE {
    ?recipient wdt:P166 wd:Q6596243 . 
    ?recipient wdt:P21 ?gender . 
  } 
  GROUP BY ?gender
}
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" . } 
} 
 ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """SELECT ?count ?gender ?genderLabel 
WITH {
  SELECT (COUNT(DISTINCT ?recipient) AS ?count) ?gender WHERE {
    ?recipient wdt:P166 wd:Q6596243 . 
    ?recipient wdt:P21 ?gender . 
  } 
  GROUP BY ?gender
} AS %result
WHERE {
  INCLUDE %result
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" . } 
} 
 ORDER BY DESC(?count)""" .
