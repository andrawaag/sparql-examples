  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d810472-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d810472-A4DCD3B4F5E8B0BA48FFD90EDB1A2669;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#Minerals by type locality
#defaultView:BubbleChart
SELECT ?locality ?localityLabel ?count
WHERE 
{
  {
    SELECT ?locality (COUNT(?mineral ) AS ?count)    
WHERE {
    ?mineral wdt:P279* wd:Q7946 . 
    ?mineral wdt:P2695 ?locality .  
        }
  GROUP BY ?locality 
  }
   SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
ORDER BY DESC(?count)
LIMIT 100""" .
