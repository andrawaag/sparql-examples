  OPTIONAL {?opera wdt:P1191 ?firstPerformanceDate}
  OPTIONAL {?opera wdt:P407 ?language}
  OPTIONAL {?opera wdt:P87 ?librettist}
  OPTIONAL {?opera wdt:P953 ?fullWork}
  ?enwiki schema:about ?opera;
