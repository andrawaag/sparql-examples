SELECT ?opera ?operaLabel 
?composerLabel 
(YEAR(?inception) AS ?year) 
?basedOn ?basedOnLabel 
?firstPerformanceDate 
?languageLabel 
?librettistLabel 
?fullWork 
?dedicatedToLabel
(URI(REPLACE(STR(?enwiki), \"/wiki/\", \"/w/api.php?action=query&prop=extracts&format=json&exintro=&titles=\")) AS ?synopsis)
