<https://www.wikidata.org/#query-ab7313cbf30aef6d4c8cd586e572c41c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2017/12>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d832546-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?itemLabel ?wikidata_groupLabel ?genderLabel ?areaLabel (CONCAT(STR(YEAR(?DoB)), 
                     \"-\", 
                     STR(MONTH(?DoB)), 
                     \"-\", 
                    STR(DAY(?DoB))) as ?displayDate)
WHERE {
  ?item p:P39 ?mem.
  ?mem ps:P39 wd:Q18607856.
  OPTIONAL { ?mem pq:P4100|pq:P102 ?wikidata_group. }
  OPTIONAL { ?mem pq:P768 ?area. }
  OPTIONAL { ?item wdt:P21 ?gender. }
  OPTIONAL { ?item wdt:P569 ?DoB. }
  FILTER NOT EXISTS { ?mem pq:P582 []. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\".}
}""" .
