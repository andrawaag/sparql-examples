    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata_talk:Events/AAFSaint%C3%8919>,
    <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2019/03>;
