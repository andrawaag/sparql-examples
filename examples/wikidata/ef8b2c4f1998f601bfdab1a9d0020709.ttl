@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <https://schema.org/> .

<https://www.wikidata.org/#query-ef8b2c4f1998f601bfdab1a9d0020709> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  rdfs:comment "Properties: position held (P39)   , occupation (P106)   , subclass of (P279)   "@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:MartinPoulter/queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d84619-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
select ?parliament ?parliamentLabel ?job ?jobLabel ?s
                    
WHERE{
  {
    SELECT ?parliament (MAX(?sens) AS ?s) WHERE{
      {
     SELECT (COUNT(?sen) AS ?sens) ?job ?parliament  WHERE { 
      ?sen wdt:P39 ?parliament;
          wdt:P106 ?job.
      ?parliament wdt:P279* wd:Q486839  
      FILTER (?job != wd:Q82955)
    } GROUP BY ?job ?parliament HAVING (?sens > 1)
  }
    } GROUP BY ?parliament
  } 
  {
     SELECT (COUNT(?sen) AS ?sens) ?job ?parliament  WHERE { 
      ?sen wdt:P39 ?parliament;
          wdt:P106 ?job.
      ?parliament wdt:P279* wd:Q486839  
      FILTER (?job != wd:Q82955)
    } GROUP BY ?job ?parliament HAVING (?sens > 1)
  }
  FILTER(?s = ?sens)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }  
} ORDER BY DESC(?s)""";
  <http://example.org/bigdata_select> """select ?parliament ?parliamentLabel ?job ?jobLabel ?s
  WITH {
     SELECT (COUNT(?sen) AS ?sens) ?job ?parliament  WHERE { 
      ?sen wdt:P39 ?parliament;
          wdt:P106 ?job.
      ?parliament wdt:P279* wd:Q486839  
      FILTER (?job != wd:Q82955)
    } GROUP BY ?job ?parliament HAVING (?sens > 1)
  } AS %subquery                  
WHERE{
  {
    SELECT ?parliament (MAX(?sens) AS ?s) WHERE{
      INCLUDE %subquery
    } GROUP BY ?parliament
  } 
  INCLUDE %subquery
  FILTER(?s = ?sens)
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\" }  
} ORDER BY DESC(?s)""" .
