<https://www.wikidata.org/#query-63fa451e329bb045c5c913844239b81c> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Misc/Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d823867-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#defaultView:BarChart
SELECT (CONCAT(STR(?genderLabel), \" / \", STR(?orientationLabel)) AS ?lab) (COUNT(*) AS ?count) WHERE {
  SELECT ?genderLabel ?orientationLabel WHERE {
    ?item wdt:P31 wd:Q5;
      wdt:P21 ?gender;
      wdt:P91 ?orientation.
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  }
}
GROUP BY ?genderLabel ?orientationLabel""" .
