  wdt:P21 wd:Q6581072 ;
  p:P2218 ?s .
  ?s rdf:type wikibase:BestRank .
  ?s psv:P2218 ?psvs .
  ?psvs wikibase:quantityAmount ?situation. 
  ?psvs wikibase:quantityUnit ?unit.
  filter (?situation >= 1000000000)
