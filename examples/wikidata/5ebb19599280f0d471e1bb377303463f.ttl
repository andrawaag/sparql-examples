<https://www.wikidata.org/#query-5ebb19599280f0d471e1bb377303463f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2023/03>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d833899-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?itemLabel (GROUP_CONCAT(DISTINCT ?val_en;separator=\"; \") as ?en) (GROUP_CONCAT(DISTINCT ?val_ru;separator=\"; \") as ?ru) (GROUP_CONCAT(DISTINCT ?val_sv;separator=\"; \") as ?sv)
WHERE 
{
  VALUES ?item {wd:Q145}
  OPTIONAL {?item wdt:P1813 ?val_en. filter(lang(?val_en) =\"en\") }
  OPTIONAL {?item wdt:P1813 ?val_ru. filter(lang(?val_ru) =\"ru\") }
  OPTIONAL {?item wdt:P1813 ?val_sv. filter(lang(?val_sv) =\"sv\") }
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". } 
} group by ?item ?itemLabel""" .
