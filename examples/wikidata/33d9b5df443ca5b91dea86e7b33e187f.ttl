<https://www.wikidata.org/#query-33d9b5df443ca5b91dea86e7b33e187f> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2019/10>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d819559-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
SELECT distinct ?item (sample (?label) as ?_samplename) (group_concat(distinct ?label) as ?name) 
WHERE 
{
  ?item wdt:P27  wd:Q801.
  ?item rdfs:label ?itemLabel .
  bind(?itemLabel as ?label) 
  filter( regex( ?itemLabel, \"\\\\wson\" )) 
} group by ?item""" .
