  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d862213-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d862213-9A3DBE2C1E7BD3E19522C4A68FED6975;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#Minerals and their commons colors
#defaultView:Graph
SELECT DISTINCT ?mineral ?mineralLabel ?rgb ?color ?colorLabel
WHERE
{
  ?mineral wdt:P279* wd:Q7946 ;
           wdt:P462 ?color .
  ?color wdt:P465 ?rgb .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}""" .
