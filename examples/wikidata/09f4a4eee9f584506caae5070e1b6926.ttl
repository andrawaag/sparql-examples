<https://www.wikidata.org/#query-09f4a4eee9f584506caae5070e1b6926> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:TweetsFactsAndQueries/Queries/most_common_properties_on_senses>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d830050-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX ontolex: <http://www.w3.org/ns/lemon/ontolex#>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?property ?propertyLabel ?count  WHERE {
  {
  SELECT ?property (COUNT(?statement) AS ?count) WHERE {
    hint:SubQuery hint:optimizer \"None\".
    ?lexeme ontolex:sense ?sense.
    ?sense ?p ?statement.
    ?property wikibase:claim ?p.
  }
  GROUP BY ?property
}.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""";
  <http://example.org/bigdata_select> """SELECT ?property ?propertyLabel ?count WITH {
  SELECT ?property (COUNT(?statement) AS ?count) WHERE {
    hint:SubQuery hint:optimizer \"None\".
    ?lexeme ontolex:sense ?sense.
    ?sense ?p ?statement.
    ?property wikibase:claim ?p.
  }
  GROUP BY ?property
} AS %results WHERE {
  INCLUDE %results.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY DESC(?count)""" .
