<https://www.wikidata.org/#query-a61e8e27ccebbf3f0d7c058758d7a933> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:WikiProject_Armenia/Showcase_Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d853414-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d853414-1914A75B7D72AB5F1322025A924A283F;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# What is the most commons birthplace in Armenia for people on Wikipedia
#defaultView:BubbleChart
SELECT ?place ?placeLabel ?count
WHERE
{
    {
        SELECT ?place (count(?item) as ?count)
        WHERE
        {
            ?item wdt:P19 ?place .
            ?place wdt:P17 wd:Q399 .
        }
        GROUP BY ?place
    }
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\" }
}
ORDER BY DESC(?count)""" .
