<https://www.wikidata.org/#query-6a1c3993dd74e1547d94785b1e654a40> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:GLAM/Baden-W%C3%BCrttemberg_GLAM_goes_OpenData/Queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d849583-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#title:\"Objekte\" des GLAM-BW-Projects mit (de)-Wikipedia-Artikel
SELECT DISTINCT ?item ?itemLabel ?wpPage WHERE {
  ?item wdt:P5008 wd:Q122901643;
    (wdt:P31/(wdt:P279+)) wd:Q386724.
  OPTIONAL { ?item wdt:P18 ?image. }
  ?wpPage schema:about ?item;
          schema:isPartOf <https://de.wikipedia.org/>
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""" .
