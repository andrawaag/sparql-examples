<https://www.wikidata.org/#query-536a575abfaa28b83db696650e1bfa1a> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Tagishsimon/prompts/Zambian_National_Assembly>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d836227-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d836227-275D4FF92F6888B123CD32D5F45ACF22;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX pr: <http://www.wikidata.org/prop/reference/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?item ?name ?constituency ?party ?gender ?birth_date ?source WHERE {
  ?item p:P39 ?mem .
  ?mem ps:P39 wd:Q18607856; pq:P2937 wd:Q45380990 .
  FILTER NOT EXISTS { ?mem pq:P582 [] }
  
  OPTIONAL { ?item p:P973/pq:P1810 ?name2 }
  OPTIONAL { ?item wdt:P21 ?genderItem }
  OPTIONAL { ?item wdt:P569 ?DoB . BIND(SUBSTR(STR(?DoB), 1, 10) AS ?birth_date) }
  OPTIONAL { ?mem pq:P4100|pq:P102 ?group }
  OPTIONAL { ?mem pq:P768 ?area }
  OPTIONAL { ?mem prov:wasDerivedFrom [ pr:P854 ?source ] }
  
  ?item rdfs:label ?name3 . FILTER(LANG(?name3) = 'en') .
  BIND(COALESCE(?name2, ?name3) AS ?name) .

   SERVICE wikibase:label { 
    bd:serviceParam wikibase:language \"en\".
    ?area rdfs:label ?constituency .
    ?group rdfs:label ?party .
    ?genderItem rdfs:label ?gender .
  }

}""" .
