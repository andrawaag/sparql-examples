WHERE {
  VALUES ?ignorableType {
    wd:Q5
    wd:Q20643955
  }
  
  VALUES ?item {
    wd:Q36322
  }
  
  ?item wdt:P31 ?type .
  
  FILTER NOT EXISTS {?item wdt:P31 ?ignorableType}
  
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
GROUP BY ?item ?itemLabel ?typeLabel
