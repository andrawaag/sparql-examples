  OPTIONAL { ?statement pq:P156 ?followed_by. FILTER (!wikibase:isSomeValue(?followed_by)) }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}
ORDER BY (UCASE(STR(?itemLabel)))""" .
