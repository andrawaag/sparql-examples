PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT DISTINCT ?item ?itemLabel ?follows ?followsLabel ?followed_by ?followed_byLabel ?statement WHERE {
  ?item wdt:P31/wdt:P279* wd:Q7889.
  ?item p:P179 ?statement.
