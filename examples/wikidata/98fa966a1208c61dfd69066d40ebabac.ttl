<https://www.wikidata.org/#query-98fa966a1208c61dfd69066d40ebabac> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Tsaorin/Subclases>, <https://www.wikidata.org//wiki/User:Tsaorin/Obrasliterarias>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d814627-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d814627-148AC3F80C06C03F474F4CD60FAA5FDD;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
SELECT (count(?item) as ?n_items) (sum(?n_arts) as ?total) 
WHERE 
{
SELECT ?item (count(?article) as ?n_arts) WHERE { 
  ?item wdt:P31 wd:Q277759 .
  ?article schema:about ?item .
  FILTER (CONTAINS(str(?article),'wikipedia'))
  }
  GROUP BY ?item
  ORDER BY DESC(?n_arts)
}""" .
