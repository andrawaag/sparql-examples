  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d822178-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d822178-E5CC988554F6B3191F313B0C2303A1A9;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?illness ?illnessLabel (count(?study) AS ?number)
WHERE {
  ?study wdt:P31 wd:Q30612 .
  ?study wdt:P1050 ?illness . 
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"de,en\" }
} GROUP BY ?illness ?illnessLabel
ORDER BY DESC(?number)""" .
