  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d853423-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d853423-3FD681544EE02A5A387673645B353274;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX schema: <http://schema.org/>
PREFIX bd: <http://www.bigdata.com/rdf#>
#Andrew Gray
SELECT ?item ?itemLabel WHERE {
    ?item wdt:P131 wd:Q22 . 
    ?item wikibase:sitelinks ?linkcount . FILTER (?linkcount = 1) .       # only include items with 1 or more sitelinks
    ?article schema:about ?item .
    ?article schema:inLanguage \"fr\" .
    ?article schema:isPartOf <https://fr.wikipedia.org/> .
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"en,fr\" }
}
GROUP BY ?item ?itemLabel""" .
