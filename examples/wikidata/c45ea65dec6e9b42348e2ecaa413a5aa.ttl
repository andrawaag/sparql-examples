<https://www.wikidata.org/#query-c45ea65dec6e9b42348e2ecaa413a5aa> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Alexmar983/Queries>;
