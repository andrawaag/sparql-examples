WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  ?essai_nucl_aire wdt:P31 wd:Q210112.
  OPTIONAL { ?essai_nucl_aire wdt:P585 ?date. }
