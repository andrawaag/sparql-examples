VALUES (?item) {(wd:Q296955) (wd:Q1348059)}
?item wdt:P279* wd:Q214609 .
?item rdfs:label ?itemLabel.
FILTER(EXISTS { FILTER((LANG(?itemLabel)) = \"en\")})
}
