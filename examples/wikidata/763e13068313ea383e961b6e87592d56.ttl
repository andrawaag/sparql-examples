    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2020/08>,
    <https://www.wikidata.org//wiki/User:Memoriav-bdc/GLAM>;
