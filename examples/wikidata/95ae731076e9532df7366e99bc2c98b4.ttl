<https://www.wikidata.org/#query-95ae731076e9532df7366e99bc2c98b4> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:Wiljes/SPARQL-queries>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d830797-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?uni ?uniLabel ?number
WHERE {
  #?uni rdfs:label ?uniLabel .
  #FILTER LANG(?uniLabel='en')
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],de,en\" }
{
SELECT DISTINCT ?uni (count(?person) AS ?number)
{
  ?person wdt:P496 ?orcid .
  ?person wdt:P108 ?uni .
  #?uni wdt:P17 wd:Q183 .    # nur Deutsche Unis
  #SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],de,en\" }
} GROUP BY ?uni
}
}
ORDER BY DESC(?number)""" .
