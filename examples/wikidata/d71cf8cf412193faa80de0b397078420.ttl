  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d8548-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d8548-9A4FFDA98F86EC3C776FFB9F6E4C733B;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
SELECT ?human ?humanLabel ?born WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],pl\". }
  ?human wdt:P31 wd:Q5.
  ?human wdt:P27 wd:Q36;
         wdt:P106 wd:Q39631.
OPTIONAL {
  ?human wdt:P569 ?born.
}  
}
GROUP BY ?human ?humanLabel ?born ORDER BY DESC(?born)
LIMIT 1000""" .
