  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d847096-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d847096-E5CFCA88439EDDAB17F1697DCA7B322A;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# Find duplicate HLS entries
SELECT ?hls (COUNT(?hls) AS ?nr_items)  (GROUP_CONCAT(?item; separator=\";\") AS ?all_items) 
WHERE
{
    ?item wdt:P902 ?hls. # DOB
    SERVICE wikibase:label { bd:serviceParam wikibase:language \"en\". }
}
GROUP BY ?hls
HAVING(COUNT(?hls) > 1)""" .
