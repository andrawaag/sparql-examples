  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d828571-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d828571-2086CB65A3323AC6A2CC26F57C3895CF;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
#defaultView:Map
SELECT DISTINCT ?item ?itemLabel ?coor WHERE {
  ?item (wdt:P31/wdt:P279*) wd:Q618123.
  ?item wdt:P17 wd:Q159.
  ?item rdfs:label ?itemLabel.
  OPTIONAL { ?item wdt:P625 ?coor . }
  MINUS {
    ?item rdfs:label ?label2.
    FILTER((LANG(?label2)) = \"ru\")
  }
}
LIMIT 1000""" .
