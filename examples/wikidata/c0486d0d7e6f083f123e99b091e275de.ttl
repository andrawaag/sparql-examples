<https://www.wikidata.org/#query-c0486d0d7e6f083f123e99b091e275de> a sh:SPARQLExecutable,
    sh:SPARQLSelectExecutable;
  dcterms:isPartOf <https://www.wikidata.org//wiki/Wikidata:Request_a_query/Archive/2019/11>;
  dcterms:license <https://creativecommons.org/licenses/by-sa/4.0/>;
  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d838717-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX ps: <http://www.wikidata.org/prop/statement/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX pr: <http://www.wikidata.org/prop/reference/>
PREFIX pq: <http://www.wikidata.org/prop/qualifier/>
PREFIX p: <http://www.wikidata.org/prop/>
SELECT ?item ?dss ?de ?fr ?it WHERE {
  ?item p:P902 ?stat.
  ?stat ps:P902 ?dss;
    prov:wasDerivedFrom ?stat2.
  {
  OPTIONAL { ?stat2 pr:P1476 ?de. filter(lang(?de)=\"de\") }
  OPTIONAL { ?stat2 pr:P1476 ?fr. filter(lang(?fr)=\"fr\") }
  OPTIONAL { ?stat2 pr:P1476 ?it. filter(lang(?it)=\"it\") }
  }
UNION
  {
  OPTIONAL { ?stat pq:P1476 ?de. filter(lang(?de)=\"de\") }
  OPTIONAL { ?stat pq:P1476 ?fr. filter(lang(?fr)=\"fr\") }
  OPTIONAL { ?stat pq:P1476 ?it. filter(lang(?it)=\"it\") }
  }
}""" .
