  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d810115-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d810115-01171DC7F9074129A68251C585348019;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX schema: <http://schema.org/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
#description particulière dans une langue donnée
SELECT DISTINCT ?item ?label
WHERE
{
  ?item schema:description \"DESCRIPTION\"@CODE DE LANGUE.
  OPTIONAL {
    ?item rdfs:label ?label.
    FILTER(LANG(?label) = \"CODE DE LANGUE DU LABEL\").
  }
}""" .
