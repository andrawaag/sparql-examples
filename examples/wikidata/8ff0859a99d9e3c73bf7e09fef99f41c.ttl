  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d842257-wikidata_prefixes;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
#defaultView:Map
SELECT DISTINCT ?person ?name ?coord_death ?layer WHERE {
  ?person wdt:P509 wd:Q84263196 .
  ?person wdt:P570 ?dod .
  ?person wdt:P20 ?place .
  ?place wdt:P625 ?coord_death .
  BIND(?dod as ?layer)
  ?person rdfs:label ?name filter (lang(?name) = \"en\")
} ORDER BY ?dod""" .
