  ?person wdt:P31 wd:Q5;
          wdt:P569 ?birth;
          wdt:P570 ?death.
  
  FILTER((\"1900-01-01\"^^xsd:dateTime <= ?birth) && (?birth < \"1910-12-31\"^^xsd:dateTime)).
  
  BIND((YEAR(?death)) - (YEAR(?birth)) AS ?age)
  FILTER((?age > 10) && (?age < 100))
  
