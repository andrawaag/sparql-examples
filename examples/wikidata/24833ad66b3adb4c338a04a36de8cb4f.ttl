SELECT ?item ?itemLabel ?koordinat_tempat ?sejak ?gambar WHERE {
  ?item wdt:P31 wd:Q32815.
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
  ?item wdt:P17 wd:Q252.
