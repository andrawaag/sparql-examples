  sh:prefixes _:genid-4e694113159d4e3db4a1a913894a81d834898-wikidata_prefixes, _:genid-4e694113159d4e3db4a1a913894a81d834898-702117E1BCEE85C2A0801D1C6C554B15;
  schema:target <https://query.wikidata.org/sparql/>;
  sh:select """PREFIX wikibase: <http://wikiba.se/ontology#>
PREFIX wdt: <http://www.wikidata.org/prop/direct/>
PREFIX wd: <http://www.wikidata.org/entity/>
PREFIX bd: <http://www.bigdata.com/rdf#>
# No image
SELECT distinct ?item ?itemLabel ?npg ?date_of_birth with 
{
  select distinct ?item where {
  {  ?item wdt:P39/wdt:P279* wd:Q16707842 . } union
  {  ?item wdt:P39/wdt:P279* wd:Q18018860 . } union
  {  ?item wdt:P39/wdt:P279* wd:Q18015642 . } union
  {  ?item wdt:P1614 ?hop } }
} as %i
where
{include %i
  filter not exists {?item wdt:P18 [] .}
  ?item wdt:P1816 ?npg. 
  OPTIONAL { ?item wdt:P569 ?date_of_birth. }
  SERVICE wikibase:label { bd:serviceParam wikibase:language \"[AUTO_LANGUAGE],en\". }
}""";
