    sh:SPARQLSelectExecutable;
  rdfs:comment "Properties: instance of (P31)   , subclass of (P279)   , publication date (P577)   , inception (P571)   , author (P50)   , language of work or name (P407)   "@en;
  dcterms:isPartOf <https://www.wikidata.org//wiki/User:MartinPoulter/queries>;
